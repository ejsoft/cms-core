{
  "name": "ejsoft/ease-cms",
  "description": "Ease CMS",
  "homepage": "https://www.ejsoft.org/ease/",
  "type": "project",
  "version": "1.0.0",
  "keywords": [
    "yii2",
    "cms",
    "ej"
  ],
  "config": {
    "process-timeout": 1800,
    "vendor-dir": "protected/vendor",
    "fxp-asset": {
      "installer-paths": {
        "npm-asset-library": "protected/vendor/npm",
        "bower-asset-library": "protected/vendor/bower"
      }
    }
  },
  "minimum-stability": "stable",
  "require": {
    "ejsoft/ease-core": "@dev",
    "ejsoft/ease-module-site": "@dev"
  }
}
