{
  "name": "cms/site",
  "description": "ej CMS",
  "homepage": "https://www.ejsoft.org/cms/",
  "type": "project",
  "version": "1.0.0",
  "keywords": [
    "yii2",
    "cms",
    "ej"
  ],
  "config": {
    "process-timeout": 1800,
    "vendor-dir": "protected/vendor",
    "fxp-asset": {
      "installer-paths": {
        "npm-asset-library": "protected/vendor/npm",
        "bower-asset-library": "protected/vendor/bower"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.0.0",
    "ejsoft/ej-core": "@dev"
  }
}
